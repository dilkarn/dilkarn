    $('.final').click(function(e){
        if(cl1 < max_val){
            alert(" send to other  "+ eval(parseInt(max_val) - parseInt(cl1)) +" Group/Friend à¤¬à¤¾à¤•à¥€ à¤¹à¥ˆà¤‚");
            e.preventDefault();
